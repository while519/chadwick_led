\pard\pardeftab720\partightenfactor0

\fs20 \cf3  opt  = \{'optimisation', 'ga', 'obj_option', 'quant_knn', 'obj_para', \{'knn', 8,8\}\};
\fs24 \cf0 \

\fs20 \cf3  
\fs24 \cf0 \

\fs20 \cf3  para_range = \{'eta1',[-1, 10],'eta2', [-1, 10],'alpha',[0, 3], 'beta',[0,3]\};
\fs24 \cf0 \

\fs20 \cf3  
\fs24 \cf0 \

\fs20 \cf3  I    = \{'R', R, 'Rq', R2,'method', 'LEDeig','dim',11, 'model_optimisation', opt, 'parameter_range',para_range \};
\fs24 \cf0 \

\fs20 \cf3  
\fs24 \cf0 \

\fs20 \cf3  led    = LEDF(I);
\fs24 \cf0 \

\fs20 \cf3  led    = training(led);\
\pard\tx566\tx1133\tx1700\tx2267\tx2834\tx3401\tx3968\tx4535\tx5102\tx5669\tx6236\tx6803\pardirnatural\partightenfactor0

